<keymap version="1" name="Default for Windows copy" parent="$default">
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl space" />
    <keyboard-shortcut first-keystroke="ctrl comma" second-keystroke="comma" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift ctrl a" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="a" />
  </action>
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="ctrl n" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="n" />
  </action>
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="shift n" />
  </action>
  <action id="GotoSymbol">
    <keyboard-shortcut first-keystroke="shift ctrl alt n" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="shift alt n" />
  </action>
  <action id="OpenFile">
    <keyboard-shortcut first-keystroke="ctrl o" />
  </action>
  <action id="OverrideMethods" />
  <action id="RecentFiles">
    <keyboard-shortcut first-keystroke="ctrl e" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="e" />
  </action>
  <action id="RecentLocations">
    <keyboard-shortcut first-keystroke="shift ctrl e" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="shift e" />
  </action>
  <action id="SmartTypeCompletion">
    <keyboard-shortcut first-keystroke="shift ctrl space" />
    <keyboard-shortcut first-keystroke="ctrl comma" second-keystroke="period" />
  </action>
  <action id="StructuralSearchPlugin.StructuralSearchAction">
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="s" />
  </action>
</keymap>