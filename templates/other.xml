<templateSet group="other">
  <template name="wnn" value="while ($VAR$ != null) {&#10;$END$&#10;}" description="Inserts 'while not null' statement" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="suggestFirstVariableName(&quot;Object&quot;)" defaultValue="var" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>